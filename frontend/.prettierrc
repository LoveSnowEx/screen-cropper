{
    "plugins": ["./node_modules/prettier-plugin-multiline-arrays"],
    "semi": false,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "endOfLine": "auto",
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "multilineArraysWrapThreshold": 10
}
